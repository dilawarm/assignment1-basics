{
    "vocab_size": 32000,
    "context_length": 512,
    "num_layers": 12,
    "d_model": 768,
    "num_heads": 12,
    "d_ff": 2048,
    "rope_theta": 10000,
    "use_squared_relu": true,
    "tie_embeddings": false,
    
    "optimizer_type": "lion",
    "weight_decay": 0.1,
    "betas": [0.95, 0.98],
    "base_lr": 0.001,
    
    "min_learning_rate": 0.00001,
    "warmup_iters": 500,
    "cosine_cycle_iters": 15000,
    
    "training_set": "data/encoded/owt_train_tokens.npy",
    "validation_set": "data/encoded/owt_valid_tokens.npy",
    
    "validation_step_interval": 200,
    "checkpoint_step_interval": 5000,
    "steps": 15000,
    "batch_size": 256,
    "gradient_accumulation_steps": 1,
    "gradient_clipping": 1.0,
    
    "use_bfloat16": true,
    "use_amp": false,
    "compile_model": true,
    "compile_mode": "reduce-overhead",
    "use_flash_attention": true,
    "use_fused_optimizers": true,
    
    "device": "cuda",
    "dataloader_num_workers": 4,
    "pin_memory": true,
    
    "experiment_name": "h100_fast_unet_transformer_stable",
    "experiment_description": "Fast U-Net transformer with stable reduce-overhead compilation",
    "use_wandb": true,
    "wandb_project": "cs336-assignment1",
    "wandb_entity": "",
    "log_dir": "experiments/h100_fast_stable"
}