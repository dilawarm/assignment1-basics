{
    "vocab_size": 32000,
    "context_length": 256,
    "num_layers": 16,
    "d_model": 1024,
    "num_heads": 8,
    "d_ff": 2816,
    "rope_theta": 10000,
    "use_squared_relu": true,
    "tie_embeddings": false,
    
    "optimizer_type": "muon_adam",
    "weight_decay": 0.01,
    "betas": [0.9, 0.999],
    "base_lr": 0.003,
    "muon_lr": 0.02,
    "muon_momentum": 0.95,
    
    "min_learning_rate": 0.0003,
    "warmup_iters": 200,
    "cosine_cycle_iters": 20000,
    
    "training_set": "data/encoded/owt_train_tokens.npy",
    "validation_set": "data/encoded/owt_valid_tokens.npy",
    
    "validation_step_interval": 200,
    "checkpoint_step_interval": 5000,
    "steps": 20000,
    "batch_size": 256,
    "gradient_accumulation_steps": 1,
    "gradient_clipping": 0.5,
    
    "use_bfloat16": true,
    "use_amp": false,
    "compile_model": true,
    "compile_mode": "reduce-overhead",
    "use_flash_attention": true,
    "use_fused_optimizers": true,
    
    "device": "cuda",
    "dataloader_num_workers": 4,
    "pin_memory": true,
    
    "experiment_name": "h100_winner_approach",
    "experiment_description": "Following winner's exact approach: 256 context, higher LR, proper optimizer setup",
    "use_wandb": false,
    "wandb_project": "cs336-assignment1",
    "wandb_entity": "",
    "log_dir": "experiments/h100_winner"
}